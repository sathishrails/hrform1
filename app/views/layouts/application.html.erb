<!DOCTYPE html>
<html>
<head>
  <title>Hrform1</title>
    <%= stylesheet_link_tag 'jquery-ui','960','sample','style' %>
    <%= javascript_include_tag 'jquery-1.5', 'jquery-ui', 'mandatescript','application','jquery.easing.1.3', 'custom' %>
    <%= csrf_meta_tag %>
</head>
<body>
<div id ="header">
	<h1><div class ="text1"><marquee><IMG SRC="/images/logo.gif" HEIGHT=28 WIDTH=100 /></marquee></div></h1>
	</div>
	<div id ="ralign">
		<p><% if user_signed_in? %>
			    <%= link_to "Sign out", destroy_user_session_path %>
			  <% else %>
			    <%= link_to "Sign up", new_user_registration_path %> or <%= link_to "sign in", 				new_user_session_path %>
		  <% end %></p>
	 </div>
<div class ="container_12" >
	<div class="grid_3">
		<ul>
			<li><h1>Home</h1><p><%= link_to 'Home', '# '%></p></li>
			<li><h1>Mandate</h1><p><%= link_to 'Mandate' , mandates_path %></p></li>
			<li><h1>Interview</h1><p><%= link_to 'Interview' , interviews_path %></p></li>
			<li><h1>Applicant</h1><p><%= link_to 'Applicant' , applicants_path %></p></li>
		</ul>
	</div>
	<div class="grid_9">
 	<%= yield %>		
	</div>
</div>
</body>
</html>
